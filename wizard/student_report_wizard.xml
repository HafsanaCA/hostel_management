<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_student_wizard_form" model="ir.ui.view">
        <field name="name">student.wizard.form</field>
        <field name="model">student.wizard</field>
        <field name="arch" type="xml">
            <form string="Students Report">
                <group>
                    <field name="room_id" />
                    <field name="student_id" />
                </group>
                <footer>
                    <button name="action_print_report" string="Print PDF " type="object" class="btn-primary"/>
                    <button name="action_print_xlsx" string="Print XLSX " type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
