<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <templates xml:space="preserve">
        <t t-name="hostel_management.category_data">
            <section class="categories_section">
                <div class="container">
                    <h3 class="section_heading">Latest Available Rooms</h3>
                    <div class="categories_wrapper">
                        <t t-foreach="result.latest_rooms" t-as="room" t-key="room.id">
                            <div class="categories">
                                <div class="type">
                                      <strong>Name:</strong> <t t-esc="room.name"/><br/>
                                      <strong>Room Type:</strong> <t t-esc="room.room_type"/><br/>
                                      <strong>No.of Beds:</strong> <t t-esc="room.beds"/><br/>
                                      <strong>Rent:</strong> <t t-esc="room.rent"/><br/>
                                      <strong>Total Rent:</strong> <t t-esc="room.total_rent"/><br/>
                                </div>
                            </div>
                        </t>
                    </div>
                </div>
            </section>
        </t>
    </templates>
</odoo>



